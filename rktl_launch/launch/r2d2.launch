<launch>
    <include file="$(find rktl_perception)/launch/camera.launch">
        <arg name="camera_name" value="cam2"/>
    </include>

    <include file="$(find rktl_perception)/launch/camera.launch">
        <arg name="camera_name" value="cam3"/>
    </include>

    <node ns="car0" pkg="rktl_perception" type="odom_generator" name="odom_generator">
        <param name="buffer_size" value="10"/>
    </node>

    <node ns="ball" pkg="rktl_perception" type="odom_generator" name="odom_generator">
        <param name="buffer_size" value="10"/>
    </node>
</launch>