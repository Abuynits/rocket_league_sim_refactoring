<?xml version="1.0"?>
<launch>
    <arg name="render" default="true"/>
    <arg name="pybullet_render" default="false"/>

    <rosparam command="load" file="$(find rocket_league_launch)/yaml/rocket_league_params.yaml"/>

    <include file="$(find rocket_league_visualization)/launch/visualizer.launch" pass_all_args="true" if="$(arg render)"/>
    <include file="$(find rocket_league_simulation)/launch/simulator.launch" pass_all_args="true"/>
</launch>
