<launch>
    <include file="$(find camera_tracking)/launch/pointgrey_camera.launch">
        <arg name="camera_name" value="cam0"/>
    </include>

    <include file="$(find camera_tracking)/launch/pointgrey_camera.launch">
        <arg name="camera_name" value="cam1"/>
    </include>

    <node pkg="camera_tracking" type="tf_parser" name="tf_parser" output="log">
        <rosparam param="camera_names">[cam0, cam1, cam2, cam3]</rosparam>
        <rosparam param="tag_names">[tag_0, tag_1, tag_2, tag_3]</rosparam>
    </node>
</launch>
