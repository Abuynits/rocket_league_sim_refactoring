<?xml version="1.0"?> 
<robot name="simplecar">
    <!-- Colors --> 
    <material name="black"> 
        <color rgba="0 0 0 1"/>
    </material>
    <material name="blue"> 
        <color rgba="0.6 0.7 0.8 1"/>
    </material>


    <!-- Base Frame of Car --> 
    <link name="base_link">
        <visual> 
            <geometry> 
                <box size="0.3 0.15 0.1"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <geometry>
            <box size=".5 0.3 0.1"/>
            </geometry>
        </collision>
        <inertial> 
            <mass value="6"/>
            <inertia ixx="0.2" ixy="0" ixz="0" iyy="0.2" iyz="0.0" izz="0.2"/>
        </inertial> 
    </link> 
    
</robot>
