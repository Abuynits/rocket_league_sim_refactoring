<launch>
  <test type="test_sync_node" pkg="rktl_control" test-name="test_sync_node"/>
  <node type="pose_synchronizer" pkg="rktl_control" name="sync_node" output="screen">
    <rosparam>
      topics:
        - car0/pose
        - car1/pose
        - ball/pose
    </rosparam>
  </node>
  <node pkg="rosservice" type="rosservice" name="set_log_level"
    args="call --wait /sync_node/set_logger_level 'rosout' 'debug'"/>
</launch>
